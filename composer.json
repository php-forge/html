{
    "name": "php-forge/html",
    "type": "library",
    "description": "Generador de c√≥digo HTML para PHP.",
    "keywords": [
        "html",
        "php"
    ],
    "license": "BSD-3-Clause",
    "prefer-stable": true,
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "roave/infection-static-analysis-plugin": "*",
        "vimeo/psalm": "*"
    },
    "autoload": {
        "psr-4": {
            "Forge\\Html\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forge\\Html\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
